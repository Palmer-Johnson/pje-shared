{
    "name": "astuteo/pje-shared",
    "description": "Shared PJE Functions",
    "type": "craft-plugin",
    "version": "2.0.0",
    "scripts": {
        "changelog": "conventional-changelog",
        "release": "conventional-changelog --commit",
        "release:patch": "conventional-changelog --patch --commit",
        "release:minor": "conventional-changelog --minor --commit",
        "release:major": "conventional-changelog --major --commit",
        "release:patch:silent": "php -d \"error_reporting=E_ALL & ~E_DEPRECATED\" ./vendor/bin/conventional-changelog --patch --commit",
        "release:minor:silent": "php -d \"error_reporting=E_ALL & ~E_DEPRECATED\" ./vendor/bin/conventional-changelog --minor --commit",
        "release:major:silent": "php -d \"error_reporting=E_ALL & ~E_DEPRECATED\" ./vendor/bin/conventional-changelog --major --commit"
    },
    "license": "proprietary",
    "authors": [
        {
            "name": "Astuteo",
            "homepage": "https://astuteo.com"
        }
    ],
    "require": {
        "craftcms/cms": "^4.0.0 || ^5.0.0",
        "skagarwal/google-places-api": "^2.0"
    },
    "require-dev": {
        "marcocesarato/php-conventional-changelog": "^1.17.2"
    },
    "extra": {
        "name": "PJE Shared",
        "handle": "pje-shared",
        "developer": "Astuteo",
        "developerUrl": "https://astuteo.com"
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    }
}
